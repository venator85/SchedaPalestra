<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="RobotoTextView">
        <attr name="typeface" />
        <attr name="min_width_ems" />
    </declare-styleable>

    <attr name="typeface" format="enum">
        <enum name="roboto_light" value="0" />
        <enum name="roboto_regular" value="1" />
        <enum name="roboto_bold" value="2" />
        <enum name="roboto_condensed_light" value="3" />
        <enum name="roboto_condensed_regular" value="4" />
        <enum name="roboto_condensed_bold" value="5" />
    </attr>
    
    <attr name="min_width_ems" format="integer" />

</resources>